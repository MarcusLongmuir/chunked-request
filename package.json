{
  "name": "chunked-request",
  "version": "0.5.4",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "repository": "https://github.com/jonnyreeves/chunked-request",
  "license": "MIT",
  "keywords": [
    "request",
    "chunked",
    "transfer",
    "comet",
    "xhr",
    "fetch"
  ],
  "scripts": {
    "prepublish": "npm run clean && npm run build:lib",
    "clean": "rm -rf build/*",
    "build:integ": "webpack",
    "build:lib": "mkdir -p lib && babel --out-dir lib src",
    "lint": "eslint .",
    "test": "npm run lint && npm run test:integ",
    "test:integ": "npm run build:integ && ./run-karma.sh --single-run",
    "release": "./release.sh ${npm_package_version}"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.13.2",
    "cookie": "^0.3.1",
    "eslint": "^3.3.1",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-sauce-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "lodash": "^4.15.0",
    "text-encoding": "^0.6.0",
    "url": "^0.11.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "browser-headers": "^0.1.6"
  }
}
